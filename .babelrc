{
  "presets": [
    ["@babel/preset-env", {
      "debug": false,
      "modules": false
    }],
    ["@babel/preset-react", {
      "useBuiltIns": true
    }]
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    }
  },
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "babel-plugin-styled-components"
  ]
}
